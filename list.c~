#include list.h

struct s_llist{
  struct llist head;
  l_cell *s_left;
  l_cell *s_right;
};

ll_cell *s_first(llist l){
  struct s_llist *cl = (struct s_llist)l;
  return cl->s_left->next;
}

ll_cell *s_last(llist l){
  struct s_llist *cl = (struct s_llist)l;
  return cl->s_left->next;
}

void *s_pop_left(llist l){
  ll_cell c = l->first(l);
  void *elem = c->head;
  l->remove_first(l);
}

void *s_pop_right(llist l){
  ll_cell c = l->first(l);
  void *elem = c->head;
  l->remove_last(l);
}
